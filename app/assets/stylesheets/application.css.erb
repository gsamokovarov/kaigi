@font-face {
  font-family: 'bt_mono';
  src: url(<%= asset_path 'bt_mono-Bold.eot' %>);
  src: url(<%= asset_path 'bt_mono-Bold.eot?#iefix' %>) format('embedded-opentype'),
       url(<%= asset_path 'bt_mono-Bold.woff2' %>) format('woff2'),
       url(<%= asset_path 'bt_mono-Bold.woff' %>) format('woff'),
       url(<%= asset_path 'bt_mono-Bold.ttf' %>) format('truetype'),
       url(<%= asset_path 'bt_mono-Bold.svg' %>#bt_monoregular) format('svg');
  font-style: normal;
  font-weight: 700; }

